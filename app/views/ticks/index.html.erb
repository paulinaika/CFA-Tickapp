<p id="notice"><%= notice %></p>

<h1>Ticks</h1>

<table>
  <thead>
    <tr>
      <th>Common name</th>
      <th>Scientific name</th>
      <th>Mpsp genotype</th>
      <th>Host species</th>
      <th>Vector</th>
      <th>Common parasite</th>
      <th>Diagnosis</th>
      <th>Description</th>
      <th>Latitude</th>
      <th>Longtitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticks.each do |tick| %>
      <tr>
        <td><%= tick.common_name %></td>
        <td><%= tick.scientific_name %></td>
        <td><%= tick.mpsp_genotype %></td>
        <td><%= tick.host_species %></td>
        <td><%= tick.vector %></td>
        <td><%= tick.common_parasite %></td>
        <td><%= tick.diagnosis %></td>
        <td><%= tick.description %></td>
        <td><%= tick.latitude %></td>
        <td><%= tick.longtitude %></td>
        <td><%= link_to 'Show', tick %></td>
        <td><%= link_to 'Edit', edit_tick_path(tick) %></td>
        <td><%= link_to 'Destroy', tick, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tick', new_tick_path %>
