<p id="notice"><%= notice %></p>

<p>
  <% @post.post_image.each do |image| %>
    <img src="<%= image.url %>">
  <% end %>
</p>

<p>
  <strong>Description:</strong>
  <%= @post.description %>
</p>

<p>
  <strong>Host Species:</strong>
  <%= @post.HostSpecies %>
</p>

<p>
  <strong>User:</strong>
  <%= @post.user.email %>
</p>

<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>
